{
    "name": "portrino/codeception-stripe-module",
    "description": "Codeception Module for Stripe API.",
    "authors": [
        {
            "name": "Andr√© Wuttig",
            "email": "andr.wuttig@gmail.com",
            "homepage": "https://github.com/aWuttig"
        }
    ],
    "require": {
        "codeception/codeception": "~2.3",
        "ocramius/package-versions": "~1.1.3",
        "stripe/stripe-php": "^7.62.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.0.2",
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codeception\\Module\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    }
}
